<interface>
  <!-- interface-name stillcenter.ui -->
  <requires lib="gtk" version="4.12"/>
  <requires lib="libadwaita" version="1.4"/>
  <object class="AdwApplicationWindow" id="window">
    <child>
      <object class="AdwNavigationView" id="main_view">
        <child>
          <object class="AdwNavigationPage" id="main_page">
            <child>
              <object class="AdwNavigationSplitView" id="split_view">
                <child>
                  <object class="AdwNavigationPage">
                    <child>
                      <object class="GtkBox" id="sidebar">
                        <property name="orientation">vertical</property>
                        <child>
                          <object class="AdwHeaderBar">
                            <property name="show-end-title-buttons">False</property>
                            <property name="title-widget">
                              <object class="AdwWindowTitle">
                                <property name="title">stillCenter</property>
                              </object>
                            </property>
                          </object>
                        </child>
                        <child>
                          <object class="GtkListBox" id="main_sidebar"/>
                        </child>
                      </object>
                    </child>
                  </object>
                </child>
                <child>
                  <object class="AdwNavigationPage">
                    <child>
                      <object class="GtkStack" id="main_stack">
                        <child>
                          <object class="GtkStackPage">
                            <property name="child">
                              <object class="GtkScrolledWindow">
                                <property name="child">
                                  <object class="GtkBox">
                                    <property name="margin-bottom">20</property>
                                    <property name="margin-end">15</property>
                                    <property name="margin-start">15</property>
                                    <property name="margin-top">20</property>
                                    <property name="orientation">vertical</property>
                                    <property name="spacing">15</property>
                                    <child>
                                      <object class="FlowApps" id="essentials"/>
                                    </child>
                                    <child>
                                      <object class="FlowApps" id="ourpicks"/>
                                    </child>
                                    <child>
                                      <object class="FlowApps" id="usedforstillos"/>
                                    </child>
                                  </object>
                                </property>
                              </object>
                            </property>
                            <property name="name">home</property>
                            <property name="title">Home</property>
                          </object>
                        </child>
                        <child>
                          <object class="GtkStackPage">
                            <property name="child">
                              <object class="GtkScrolledWindow"/>
                            </property>
                          </object>
                        </child>
                      </object>
                    </child>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
        <style/>
      </object>
    </child>
  </object>
</interface>
